sbom/
*.sbom
*.spdx
*.cyclonedx

# Vulnerability scan reports
scan-results/
*.scan
grype-report.*
trivy-report.*

# Container runtime artifacts
.docker/
.podman/

# Local testing
test-ee/
local-ee/

